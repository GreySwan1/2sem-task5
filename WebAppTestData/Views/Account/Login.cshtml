
@{
    ViewData["Title"] = "Login account";
}

<h1>Login account</h1>

<form asp-action="Login">
    <div>
        <label>
            Login (email):
        </label>
        <input class="form-control"/>
    </div>
    <div>
        <label>
            Password:
        </label>
        <input class="form-control"/>
    </div>
    <button type="submit" class="btn btn-outline-success sign_in" onclick="signIn" asp-controller="Home" asp-action="Index">Log in</button>
</form>